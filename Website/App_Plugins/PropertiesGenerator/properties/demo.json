{
  "sections": [
    {
      "name": "Validations",
      "alias": "validations",
      "properties": [
        {
          "name": "Required",
          "alias": "required",
          "type": "Textbox",
          "validate": {
            "required": true,
            "requiredMessage": "this field is required"
          }
        },
        {
          "name": "Regex",
          "alias": "regex",
          "type": "Textbox",
          "validate": {
            "regex": "^e",
            "regexMessage": "Must start with 'e"
          }
        },
        {
          "name": "Custom function",
          "alias": "customFunction",
          "type": "Textbox",
          "validate": {
            "customFunction": "function(val, model) { if(val != 'a') return 'value mast be \"a\"'; return null; }"
          }
        }
      ]
    },
